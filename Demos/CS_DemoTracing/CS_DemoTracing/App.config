<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8.1" />
    </startup>
	<system.diagnostics>
		<trace autoflush="true">
			<listeners>
				<clear/>
				<add name="textWriterTraceListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="CS_DemoTracing.log" />
				<add name="consoleTraceListener" type="System.Diagnostics.ConsoleTraceListener" />
			</listeners>
		</trace>
	</system.diagnostics>
</configuration>